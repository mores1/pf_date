<div class="spot">
  <% if @spot.present? %>
    <%= render 'tag_index', spot: @spot %>
  <% elsif @check.present? %>
    <%= render 'search_index', search_spots: @search_spots %>
  <% else %>
    <%= render 'all_index', spots: @spots %>
  <% end %>
  <%= paginate @spots %>
</div>
<script src="https://maps.googleapis.com/maps/api/js?key=<%= ENV['MAP_KEY'] %>&callback=initMap" async defer></script>